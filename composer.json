{
    "name": "ufee/rfc6455",
    "type": "library",
    "description": "RFC6455 WebSocket protocol handler",
    "keywords": ["WebSocket", "RFC6455"],
    "homepage": "http://socketo.me",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Boden"
          , "email": "cboden@gmail.com"
          , "role": "Developer"
        },
        {
            "name": "Matt Bonneau",
            "role": "Developer"
        },
        {
            "name": "Chris van Steenbergen",
            "homepage": "https://github.com/dcnl1980",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ufee"
    },
    "autoload": {
        "psr-4": {
            "Ratchet\\RFC6455\\": "src"
        }
    },
    "require": {
        "php": ">=5.4.2 || ^7.0 || ^8.0",
        "guzzlehttp/psr7": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "react/socket": "^1",
    },
    "scripts": {
        "abtest-client": "ABTEST=client && sh tests/ab/run_ab_tests.sh",
        "abtest-server": "ABTEST=server && sh tests/ab/run_ab_tests.sh",
        "phpunit": "phpunit --colors=always",
        "test": [
            "@abtest-client",
            "@abtest-server",
            "@phpunit"
        ]
    }
}
